{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:import/typescript",
    "plugin:jest-dom/recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "2021",
    "sourceType": "module"
  },
  "plugins": [
    "import",
    "jest-dom",
    "react",
    "react-hooks",
    "@typescript-eslint"
  ],
  "rules": {
    "import/no-default-export": 2,
    "react/react-in-jsx-scope": "off"
  },
  "settings": {
    "import/resolver": {
      "node": true,
      "typescript": true
    },
    "react": {
      "version": "detect"
    }
  }
}
